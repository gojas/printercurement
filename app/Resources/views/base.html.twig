<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>

        {% block body %} {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.11.2.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('js/angular.min.js') }}"></script>
            <script src="{{ asset('js/angular-route.min.js') }}"></script>
            <script src="{{ asset('js/ui-bootstrap-tpls-0.12.1.min.js') }}"></script>
            <script src="{{ asset('js/highcharts/js/highcharts.js') }}"></script>
            <script src="{{ asset('js/highcharts/js/highcharts-more.js') }}"></script>
            <script src="{{ asset('js/highcharts/js/modules/exporting.js') }}"></script>

            {% javascripts %}

            {% endjavascripts %}
        {% endblock %}

    </body>
</html>
